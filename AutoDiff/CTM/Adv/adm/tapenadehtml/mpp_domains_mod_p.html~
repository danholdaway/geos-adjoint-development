    <a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="typename">TYPE(DOMAIN2D)</code>, <code class="typename">INTENT(IN) </code>:: <code class="vardecl">domain</code></a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="typename">REAL(</code><code class="modifier">real8</code><code class="typename">)</code>, <code class="typename">OPTIONAL</code>, <code class="typename">INTENT(INOUT) </code>:: <code class="vardecl">wbuffery</code>(:, :), <code class="vardecl">ebuffery</code>(:, <code class="label">&</code></a>
<a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="label">&   </code>:), <code class="vardecl">sbufferx</code>(:, :), <code class="vardecl">nbufferx</code>(:, :)</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="typename">REAL(</code><code class="modifier">real8</code><code class="typename">)</code>, <code class="typename">OPTIONAL</code>, <code class="typename">INTENT(INOUT) </code>:: <code class="vardecl">wbufferx</code>(:, :), <code class="vardecl">ebufferx</code>(:, <code class="label">&</code></a>
<a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="label">&   </code>:), <code class="vardecl">sbuffery</code>(:, :), <code class="vardecl">nbuffery</code>(:, :)</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="typename">INTEGER</code>, <code class="typename">OPTIONAL</code>, <code class="typename">INTENT(IN) </code>:: <code class="vardecl">gridtype</code></a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile">field1 = <code class="constant">2</code>*field1</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile">field2 = <code class="constant">2</code>*field2</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile">wbuffery(:, :) = field1(<code class="constant">1</code>, :, :)</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile">ebuffery(:, :) = field1(<code class="constant">2</code>, :, :)</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile">sbufferx(:, :) = field2(<code class="constant">1</code>, :, :)</a>
    <a href="null.html#mpp_domains_mod_b" target="diffFile">nbufferx(:, :) = field2(<code class="constant">2</code>, :, :)</a>
  <a href="null.html#mpp_domains_mod_b" target="diffFile"><code class="keyword">END SUBROUTINE </code><code class="funcname">MPP_GET_BOUNDARY</code></a>
